{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "spotifyDFV2"
		},
		"adls_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'adls_ls'"
		},
		"source_sql_ls_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'source_sql_ls'"
		},
		"adls_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://spotifyadlsv2.dfs.core.windows.net/"
		},
		"source_sql_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "spotifyserverv1.database.windows.net"
		},
		"source_sql_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "spotifydb"
		},
		"source_sql_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "adminmanas"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "spotifyDFV2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"adls_ls_accountKey": {
						"value": "[parameters('adls_ls_accountKey')]"
					},
					"source_sql_ls_password": {
						"value": "[parameters('source_sql_ls_password')]"
					},
					"adls_ls_properties_typeProperties_url": {
						"value": "[parameters('adls_ls_properties_typeProperties_url')]"
					},
					"source_sql_ls_properties_typeProperties_server": {
						"value": "[parameters('source_sql_ls_properties_typeProperties_server')]"
					},
					"source_sql_ls_properties_typeProperties_database": {
						"value": "[parameters('source_sql_ls_properties_typeProperties_database')]"
					},
					"source_sql_ls_properties_typeProperties_userName": {
						"value": "[parameters('source_sql_ls_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}